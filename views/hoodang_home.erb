<div id="container">
  <div id="toolbar"><a href="/">home</a></div>
  <div id="header"><img class="avatar" /></div>
  <div id="flag-container">
    <ul>
      <% @users.each do |user| %>
      <li id="<%= user.id %>" class="<%= user.status %>">
        <img src="<%= user.image_url %>" alt="user image">
        <%= user.name %>
      </li>
      <% end %>
    </ul>
  </div>
</div>